{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "func-names": 0,
    "indent": [2, 4, { "SwitchCase": 1 }],
    "prefer-arrow-callback": 0,
    "prefer-const": 0
  }
}
